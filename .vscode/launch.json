{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Waffle",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "cwd": "${workspaceFolder}/build",
            "output": "build/Waffle.exe",
            "asRoot": true,
            "console": "integratedTerminal"
        },
        {
            "name": "Launch osz Renamer",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "cwd": "${workspaceFolder}/build",
            "output": "build/Waffle.exe",
            "args": [
                "osz_renamer",
                "${workspaceFolder}/build/oszs"
            ],
            "asRoot": true,
            "console": "integratedTerminal"
        },
        {
            "name": "Launch .osu Mover",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "cwd": "${workspaceFolder}/build",
            "output": "build/Waffle.exe",
            "args": [
                "osu_mover",
                "V:/osu!waffle map holder/Songs"
            ],
            "asRoot": true,
            "console": "integratedTerminal"
        },
        {
            "name": "Launch Necessary Migrations",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "cwd": "${workspaceFolder}/build",
            "output": "build/Waffle.exe",
            "args": [
                "migrate",
            ],
            "asRoot": true,
            "console": "integratedTerminal"
        },
        {
            "name": "Launch Beatmap Versioning",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "cwd": "${workspaceFolder}/build",
            "output": "build/Waffle.exe",
            "args": [
                "beatmap_versions",
                "./osus"
            ],
            "asRoot": true,
            "console": "integratedTerminal"
        }
    ]
}